  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jota-one/drosse-ui/issues"
  },
  "homepage": "https://drosse.dev",
  "dependencies": {
    "concurrently": "^5.2.0",
    "core-js": "^3.6.5",
    "get-port": "^5.1.1",
    "node-discover": "^1.1.3",
    "sockjs": "^0.3.20",
    "sockjs-client": "^1.4.0",
    "vue": "^3.0.0-beta.18"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.0",
    "@vue/cli-plugin-eslint": "^4.4.0",
